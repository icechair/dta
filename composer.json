{
  "name": "icechair/dta",
  "description": "DTA File exporter",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Gabel",
      "email": "martin.gabel@icechair.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "icechair\\": "icechair/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "icechair\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/icechair/dta"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "phpunit/php-code-coverage": "^3.3"
  },
  "scripts":{
      "test": [
          "phpdbg -qrr /usr/bin/phpunit --bootstrap ./vendor/autoload.php --coverage-html coverage"
      ]
  }
}
